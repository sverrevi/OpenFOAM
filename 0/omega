FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 625;

boundaryField
{


    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    outlet
    {
        type            zeroGradient;
    }

	
    slipwalls
    {
        type            slip;
    }
	
	top
    {
        type            symmetry;
    }

    oar
    {
        type            omegaWallFunction;
        value           $internalField;
	}


}


// ************************************************************************* //
